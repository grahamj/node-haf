{
  "env": { "node": true },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": { "ecmaVersion": 14 },
  "rules": {
    "object-curly-newline": ["error", { "multiline": true, "consistent": true } ],
    "max-len": 0,
    "padded-blocks": 0,
    "arrow-body-style": 0,
    "keyword-spacing": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "import/extensions": 0
  }
}
